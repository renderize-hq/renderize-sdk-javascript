{
  "name": "@renderize/lib",
  "type": "module",
  "version": "1.0.2",
  "packageManager": "pnpm@9.12.3",
  "description": "Renderize SDK for JavaScript and TypeScript",
  "author": "Renderize <pnpm@renderize.tech> (https://renderize.tech)",
  "license": "MIT",
  "homepage": "https://renderize.tech",
  "repository": "github:renderize-hq/renderize-sdk-javascript",
  "bugs": {
    "url": "https://github.com/renderize-hq/renderize-sdk-javascript/issues"
  },

  "keywords": [
    "renderize",
    "sdk",
    "api",
    "pdf",
    "rendering",
    "html",
    "css"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "prepare": "pnpm run build",
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "pnpm run test:unit",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --tag --push"
  },
  "dependencies": {
    "ofetch": "^1.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.6.2",
    "@types/node": "^22.5.4",
    "@vitest/coverage-v8": "^2.0.5",
    "bumpp": "^9.8.1",
    "dotenv": "^16.4.5",
    "eslint": "^9.10.0",
    "tsx": "^4.17.0",
    "typescript": "^5.6.2",
    "unbuild": "^2.0.0",
    "vitest": "^2.0.5"
  }
}
